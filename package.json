{
  "name": "@infodb/skimalens",
  "version": "1.0.0",
  "description": "A data visualization tool for JSON and YAML files, specifically designed to view and analyze Claude and ChatGPT conversation logs",
  "keywords": [
    "claude",
    "chatgpt",
    "conversation",
    "data-visualization",
    "json-viewer",
    "yaml-viewer",
    "cli-tool",
    "data-analysis",
    "typescript",
    "react"
  ],
  "homepage": "https://github.com/infodb/skimalens#readme",
  "bugs": {
    "url": "https://github.com/infodb/skimalens/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infodb/skimalens.git"
  },
  "license": "MIT",
  "author": "tamuto <tamuto@infodb.jp>",
  "main": "dist/index.js",
  "bin": {
    "skimalens": "./bin/cli.ts"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
